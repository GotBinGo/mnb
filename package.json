{
  "name": "autsofthub",
  "version": "0.5.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --proxy-config proxy.conf.js",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "e2e": "ng e2e",
    "prettier": "prettier --write \"./{src,e2e}/**/*.{ts,js,html,scss}\"",
    "prettier:check": "prettier --list-different \"./{src,e2e}/**/*.{ts,js,html,scss}\"",
    "precommit": "pretty-quick --config .prettierrc --staged",
    "swagger": "nswag run angularclient.nswag /runtime:NetCore30"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@fortawesome/fontawesome-free": "^5.13.0",
    "@ng-bootstrap/ng-bootstrap": "^5.3.0",
    "@ng-select/ng-select": "^3.7.3",
    "@rxweb/reactive-form-validators": "^1.9.8",
    "angular2-uuid": "^1.1.1",
    "bootstrap": "^4.4.1",
    "file-saver": "^2.0.2",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.15",
    "ngx-image-cropper": "^2.1.2",
    "ngx-sortablejs": "^3.1.4",
    "ngx-toastr": "^11.3.3",
    "rxjs": "^6.5.4",
    "sortablejs": "^1.10.2",
    "three": "^0.109.0",
    "three-gltf-loader": "^1.111.0",
    "tslib": "^1.11.1",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.25",
    "@angular/cli": "^8.3.25",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@angularclass/hmr": "^2.1.3",
    "@types/file-saver": "^2.0.1",
    "@types/jasmine": "^3.5.10",
    "@types/jasminewd2": "~2.0.8",
    "@types/jwt-decode": "^2.2.1",
    "@types/lodash": "^4.14.149",
    "@types/node": "~8.9.4",
    "@types/sortablejs": "^1.10.3",
    "@types/three": "^0.103.2",
    "codelyzer": "^5.2.2",
    "htmlhint": "^0.11.0",
    "husky": "^3.1.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.5.3",
    "nswag": "^13.3.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "protractor": "^5.4.3",
    "stylelint": "^12.0.1",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-recommended-scss": "~4.1.0",
    "stylelint-config-standard": "~19.0.0",
    "stylelint-scss": "~3.13.0",
    "ts-node": "~8.4.1",
    "tslint": "~5.20.1",
    "tslint-config-prettier": "^1.18.0",
